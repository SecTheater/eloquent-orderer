{
    "name": "sectheater/eloquent-orderer",
    "description": "Simple package to order items with bearing in mind scalability",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohammed Osama",
            "email": "mohammedosama@ieee.org"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "laravel/framework": "~5.8.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^8.0",
        "orchestra/testbench": "~3.8.0|^4.0"
    },
    "autoload": {
        "psr-4": {
            "Eloquent\\Orderer\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eloquent\\Orderer\\Tests\\": "tests"
        }
    },
    "keywords": [
        "sort",
        "sortable",
        "eloquent",
        "model",
        "laravel"
    ]
}